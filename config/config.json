{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "incentives-dashboard-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/incentivesDashboard/IncentivesDashboardWebPart.js",
          "manifest": "./src/webparts/incentivesDashboard/IncentivesDashboardWebPart.manifest.json"
        }
      ]
    },
    "incentive-analytics-header-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/incentiveAnalyticsHeader/IncentiveAnalyticsHeaderWebPart.js",
          "manifest": "./src/webparts/incentiveAnalyticsHeader/IncentiveAnalyticsHeaderWebPart.manifest.json"
        }
      ]
    },
    "home-links-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/homeLinks/HomeLinksWebPart.js",
          "manifest": "./src/webparts/homeLinks/HomeLinksWebPart.manifest.json"
        }
      ]
    },
    "incentive-simulator-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/incentiveSimulator/IncentiveSimulatorWebPart.js",
          "manifest": "./src/webparts/incentiveSimulator/IncentiveSimulatorWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "IncentivesDashboardWebPartStrings": "lib/webparts/incentivesDashboard/loc/{locale}.js",
    "IncentiveAnalyticsHeaderWebPartStrings": "lib/webparts/incentiveAnalyticsHeader/loc/{locale}.js",
    "HomeLinksWebPartStrings": "lib/webparts/homeLinks/loc/{locale}.js",
    "IncentiveSimulatorWebPartStrings": "lib/webparts/incentiveSimulator/loc/{locale}.js"
  }
}
